%h1 Listing teams
%table
  %tr
    %th leader
    %th Competition
    %th Name
    %th Team
    %th
    %th
    %th
  - @teams.each do |team|
    %tr
      %td= user_name(team.leader)
      %td= team.name
      %td= link_to 'Show', team
      %td= link_to 'Edit', edit_team_path(team)
      %td= link_to 'Destroy', team, confirm: 'Are you sure?', method: :delete
%br/
= link_to 'New Team', new_team_path
